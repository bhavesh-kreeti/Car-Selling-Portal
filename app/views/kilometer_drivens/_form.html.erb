

<%= form_with(model: kilometerdriven, local: true) do |form| %>
   <%= render 'shared/error', obj: @kilometerdriven %>


  <div class="field card-body">
    <%= form.label :name  %>
    <%= form.text_field :name, placeholder: 'Enter City Name' %>
  </div>

  <div class="actions text-center">
    <%= form.submit class: 'btn btn-success' %>
  </div>
<% end %>
